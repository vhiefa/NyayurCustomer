<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vhiefa.nyayur">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.SplashscreenActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.BelanjaActivity" />
        <activity android:name=".activity.DaftarTukangActivity" />
        <activity android:name=".activity.PilihTukangActivity" />
        <activity android:name=".activity.ProfilActivity" />
        <activity android:name=".activity.UbahProfilActivity" />
        <activity android:name=".activity.SubCategoryActivity"></activity>
    </application>

</manifest>